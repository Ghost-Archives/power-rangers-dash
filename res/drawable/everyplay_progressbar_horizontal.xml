<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#00122f83" android:endColor="#00122f83" android:angle="270.0" android:centerY="0.5" android:centerColor="#00122f83" />
